page {
  overflow: hidden;
}
page .main {
  position: relative;
  overflow: hidden;
  width: 100%;
  padding: 35rpx;
}
page .main .topMask {
  position: absolute;
  width: 120%;
  left: -10%;
  height: 400rpx;
  border-radius: 0 0 50% 50%;
  background: -webkit-linear-gradient(#c43529, #dd4730);
  background: linear-gradient(#c43529, #dd4730);
  top: 0;
  z-index: -1;
}
page .main .armsList {
  padding-bottom: 20rpx;
}
page .main .armsList .li {
  width: 33.33%;
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  vertical-align: top;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding-bottom: 20rpx;
}
page .main .armsList .li image {
  width: 200rpx;
  height: 60rpx;
  border-radius: 10rpx;
}
page .main .armsList .li.act image {
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.6);
}
page .main .swiper {
  width: 100%;
  height: 300rpx;
}
page .main .swiper swiper {
  width: 100%;
  height: 300rpx;
}
page .main .swiper swiper .swiper-item {
  width: 100%;
  height: 300rpx;
  position: relative;
}
page .main .swiper swiper .swiper-item image {
  width: 100%;
  height: 100%;
  border-radius: 25rpx;
}
page .main .swiper swiper .swiper-item .mask {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  padding: 45rpx 20rpx;
  font-size: 24rpx;
}
page .main .swiper swiper .swiper-item .mask .name {
  font-size: 28rpx;
}
page .main .list .title {
  font-size: 24rpx;
  color: #000;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  background: #fff;
  padding: 20rpx 0;
  width: 100%;
  z-index: 9;
}
page .main .list .title::before {
  content: '';
  display: inline-block;
  width: 10rpx;
  background: #d73f31;
  height: 20rpx;
  margin-right: 10rpx;
  vertical-align: baseline;
}
page .main .list .partsList {
  width: 100%;
}
page .main .list .partsList .li {
  width: 25%;
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  color: #080808;
  font-size: 20rpx;
}
page .main .list .partsList .li image {
  width: 100rpx;
  height: 100rpx;
  border: 5rpx solid #C0C0C0;
}
page .main .list .partsList .li .name {
  padding-top: 10rpx;
}
page .main .list .numerical {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 22rpx;
  color: #080808;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
page .main .list .numerical .valList {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
}
page .main .list .numerical .valList + .valList {
  padding-top: 20rpx;
}
page .main .list .numerical .valList .valName {
  width: 20%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
page .main .list .numerical .valList .middle {
  width: 60%;
  padding: 0 10rpx;
}
page .main .list .numerical .valList .middle .per {
  width: 100%;
  height: 50rpx;
  background: #C0C0C0;
  position: relative;
}
page .main .list .numerical .valList .middle .per .act {
  position: absolute;
  top: 0;
  left: 0;
  width: 50%;
  height: 100%;
  background: #c43529;
}
page .main .list .numerical .valList .valNum {
  width: 20%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
page .main .list .advantage,
page .main .list .shortcoming {
  font-size: 22rpx;
  color: #080808;
  padding-bottom: 20rpx;
}

